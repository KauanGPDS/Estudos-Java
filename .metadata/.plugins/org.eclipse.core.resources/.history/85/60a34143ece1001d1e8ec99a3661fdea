package orietanacao;

public class ProdutoTeste {
	
	String nome;
	double preco;
	double desconto;
	ProdutoTeste(String nomeIncial){
		nome = nomeIncial;
		
	}
	ProdutoTeste(String nomeIncial,double precoInicial){
		nome = nomeIncial;
		preco = precoInicial;
		
	}
	
	double precoComDesconto(double descontoDoGerente) {
		return (preco*(desconto+descontoDoGerente))/100;
	}

}
