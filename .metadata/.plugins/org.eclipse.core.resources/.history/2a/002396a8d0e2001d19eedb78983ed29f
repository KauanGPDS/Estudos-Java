package orietanacao;

public class data {
	int dia;
	int mes;
	int ano;
	
	
	data(){ 
		//dia = 1;
		//mes = 1;
		//ano = 1970;
		this(1,1,1970);
	}
	data(int dia,int mes,int ano){
		this.dia = dia;
		this.mes =mes;
		this.ano = ano;
		
	}
	String dataFormatada() {
		return String.format("%d/%d/%d\n",dia,mes,ano);
	}
	void ImprimirDataFormata() {
		System.out.println(this.dataFormatada());
	}
		
	}


