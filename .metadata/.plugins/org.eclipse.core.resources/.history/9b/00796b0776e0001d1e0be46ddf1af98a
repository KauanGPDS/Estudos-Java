package orietanacao;

public class Produto {
	public static void main(String[] args) {
		
		ProdutoTeste p1 = new ProdutoTeste();
		p1.nome = "note";
		p1.preco = 4325.89;
		p1.desconto = 0.25;
		
		var p2 = new ProdutoTeste();
		
		p2.nome = "Caneta Preta";
		p2.preco = 12.56;
		p2.desconto = 0.29;
		
		System.out.println(p1.nome);
		System.out.println(p2.nome);
		
		double precofinal = p1.precoComDesconto();
		double precofinal1 = p2.precoComDesconto();
		
		System.out.printf("Media do Carrinho = R$%.2f.",precofinal);

}
}
