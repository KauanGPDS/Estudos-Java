package ClassEModulo;

public class Data {
	public static void main(String[] args) {
		dataTeste d1 = new dataTeste();
		d1.ano = 1990;
		d1.dia = 27;
		d1.mes = 03;
		
		var d2 = new dataTeste();
		d2.ano = 1930;
		d2.dia = 01;
		d2.mes = 12;
		
		System.out.println(d2.dia + "/" + d2.mes + "/" + d2.ano);
		
	}

}
