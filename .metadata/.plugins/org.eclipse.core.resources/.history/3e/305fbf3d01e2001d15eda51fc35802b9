package orietanacao;

public class AreaCircTeste {
	public static void main(String[] args) {
		AreaCirc a = new AreaCirc(10);
		//a.pi = 10;
	
		
		AreaCirc a1 = new AreaCirc(10);
		//a1.pi = 5;
		
		AreaCirc.pi = 3.1415;
		
		System.out.println(a1.area());
		
		AreaCirc.pi = 0.1;
		System.out.println(a.area());
	}
	

}
